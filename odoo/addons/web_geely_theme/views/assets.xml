<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="_assets_primary_variables" inherit_id="web._assets_primary_variables" priority="15">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/primary_variables.scss"/>
            </xpath>
        </template>

        <template id="assets_common" name="common assets" inherit_id="web.assets_common">
            <xpath expr="//script[@src='/web/static/src/js/services/config.js']" position="replace">
                <script type="text/javascript" src="/web_geely_theme/static/src/js/config.js"/>
            </xpath>
        </template>

        <template id="assets_backend_prod_only" inherit_id="web.assets_backend_prod_only">
            <xpath expr="//script[@src='/web/static/src/js/main.js']" position="replace">
                <script type="text/javascript" src="/web_geely_theme/static/src/js/main.js"/>
            </xpath>
        </template>

        <template id="assets_backend" name="web backend assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/form_view.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/scroll.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/webclient_layout.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/navbar.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/sidebar_menu.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/home_menu.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/favorite_menu.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/search_view.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/pager.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/list_view.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/modal.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/fields.scss"/>
                <link rel="stylesheet" type="text/scss" href="/web_geely_theme/static/src/scss/data_export.scss"/>

                <script type="text/javascript" src="/web_geely_theme/static/src/js/list_renderer.js"/>
                <script type="text/javascript" src="/web_geely_theme/static/src/js/form_controller.js"/>
                <script type="text/javascript" src="/web_geely_theme/static/src/js/action_manager.js"/>
                <script type="text/javascript" src="/web_geely_theme/static/src/js/menu.js"/>
                <script type="text/javascript" src="/web_geely_theme/static/src/js/user_menu.js"/>
                <script type="text/javascript" src="/web_geely_theme/static/src/js/sidebar_menu.js"/>
                <script type="text/javascript" src="/web_geely_theme/static/src/js/home_menu.js"/>
                <script type="text/javascript" src="/web_geely_theme/static/src/js/web_client.js"/>
                <script type="text/javascript" src="/web_geely_theme/static/src/js/favorite_menu.js"/>

            </xpath>
        </template>
    </data>
</odoo>
